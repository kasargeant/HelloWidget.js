            "checkRedundantReturns": true,
            "requireReturnTypes": true,
            "checkTypes": true,
            "checkRedundantAccess": true
        }
    },
    "jshintConfig": {
        "undef": true,
        "unused": true,
        "predef": [ "self" ]
    },
    "scripts": {
        "lint": "lint.bat",
        "test": "echo \"Error: no test specified\" && exit 1"
