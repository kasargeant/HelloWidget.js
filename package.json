    "main": "src/HelloWidget.js",
    "directories": {
        "test": "test"
    },
    "devDependencies": {
        "jscs": "2.6.0",
        "jshint": "2.8.0"
    },
    "jscsConfig": {
        "preset": "google",
        "validateQuoteMarks": { "mark": "\"", "escape": true },
        "validateIndentation": 4,
        "jsDoc": {
            "checkAnnotations": "closurecompiler",
            "checkParamNames": true,
            "requireParamTypes": true,
            "checkRedundantParams": true,
